<div class="row">
    <div class="col-lg-12">
        <div class="card">
            <div class="card-header">
                <p>Data batch sentiment analisis</p>
            </div>
            <div class="card-body">
                <table class="table table-striped">
                    <thead>
                        <tr>
                            <th scope="col">#</th>
                            <th scope="col">Batch ID</th>
                            <th scope="col">Date</th>
                            <th scope="col">Persentase</th>
                            <th scope="col">Status</th>
                            <th scope="col">Action</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% var count=0 %> 
                        <% if(typeof data != 'undefined' && data) { %> 
                        <% data.forEach(data => { %> 
                        <tr>
                            <% count++ %> 
                            <th scope="row"><%= count %> </th>
                            <td><%= data.name %> </td>
                            <td><%= data.createdAt %> </td>
                            <td>P: <%= data.persentase_positive%>% - Ne: <%= data.persentase_negative%>% - Nt: <%= data.persentase_netral%>% </td>
                            <td>
                                <% if(data.status == 'selesai') { %>
                                <a href="#" class="badge badge-primary">Selesai</a>
                                <% } else { %> 
                                <a href="#" class="badge badge-warning">On Progress</a>
                                <% } %> 
                            </td>
                            <td><a href="/list-batch/<%= data.id %> " class="btn btn-sm btn-info">Show Sentiments</a></td>
                        </tr>
                        <% }) %> 
                        <% } %> 
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>